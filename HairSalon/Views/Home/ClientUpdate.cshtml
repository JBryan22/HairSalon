@{
    Layout = "_Layout";
}


<h2 class="headline">Update Client</h2>

<form class="form-group" action='/stylist/@Model["stylists"].GetId()/clients/@Model["clients"].GetId()/update' method="post">
    <label for="client-name">Name:</label>
    <input type="text" name="client-name" class="form-control">

    <button type="submit" class="btn btn-default">Submit</button>
</form>
